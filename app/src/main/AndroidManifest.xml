<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="calvoitmove.projett">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectChannelActivity"
            android:label="@string/select_channel_title"></activity>
        <activity
            android:name=".CreateChannelActivity"
            android:label="@string/create_channel_title"
            android:parentActivityName=".SelectChannelActivity"></activity>
        <activity
            android:name=".JoinChannelActivity"
            android:label="@string/join_channel_title"
            android:parentActivityName=".SelectChannelActivity"></activity>
        <activity android:name=".ChannelActivity"></activity>
    </application>

</manifest>